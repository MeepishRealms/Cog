	"player_relation_to_passives": "HOSTILE",
	// Relation to unspecified entities those are instance of HostileEntity(Yarn)
	"player_relation_to_hostiles": "HOSTILE",
	// Fallback relation
	"player_relation_to_other": "HOSTILE"
